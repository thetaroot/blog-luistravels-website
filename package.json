{
  "name": "personal-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "node scripts/generate-content-stats.js && node scripts/generate-sitemap.js && next build",
    "start": "next start",
    "lint": "next lint",
    "update-content": "node scripts/generate-content-stats.js",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "seo-audit": "node scripts/generate-sitemap.js && echo 'SEO audit complete - check sitemap.xml and sitemap-stats.json'",
    "phase7": "tsx src/lib/cli/phase7-command.ts",
    "phase7:status": "tsx src/lib/cli/phase7-command.ts",
    "phase7:batch": "tsx src/lib/cli/phase7-command.ts --batch",
    "google-10-of-10": "tsx src/lib/cli/phase7-command.ts --batch --target=100 --aggressive=true",
    "phase8": "tsx src/lib/cli/phase8-command.ts",
    "phase8:status": "tsx src/lib/cli/phase8-command.ts",
    "phase8:batch": "tsx src/lib/cli/phase8-command.ts --batch",
    "seo-dominance": "tsx src/lib/cli/phase8-command.ts --batch --aggressive=true",
    "phase9": "tsx src/lib/cli/phase9-command.ts",
    "phase9:all": "tsx src/lib/cli/phase9-command.ts --all",
    "phase9:auto": "tsx src/lib/cli/phase9-command.ts --all --auto-apply",
    "phase9:strict": "tsx src/lib/cli/phase9-command.ts --all --strict-mode --auto-apply",
    "google-10-of-10-qa": "tsx src/lib/cli/phase9-command.ts --all --auto-apply --target-score 95 --verbose",
    "seo-perfection": "tsx src/lib/cli/phase9-command.ts --all --auto-apply --strict-mode --target-score 98"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@types/dompurify": "^3.0.5",
    "@use-gesture/react": "^10.3.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.6",
    "framer-motion": "^10.16.0",
    "glob": "^10.4.5",
    "gray-matter": "^4.0.3",
    "html-to-image": "^1.11.13",
    "lucide-react": "^0.536.0",
    "next": "14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-simple-maps": "^3.0.0",
    "react-spring": "^9.7.5",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-html": "^16.0.1",
    "suspend-react": "^0.1.3",
    "three": "^0.159.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-simple-maps": "^3.0.6",
    "autoprefixer": "^10.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "14.2.5",
    "postcss": "^8.0.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0"
  }
}
